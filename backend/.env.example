
# Oracle Database Configuration
# ----------------------------
# Replace with your Oracle credentials
DB_USER=your_oracle_username
DB_PASSWORD=your_oracle_password

# Format examples:
# - Local Oracle XE: localhost:1521/XE
# - Oracle Cloud: hostname.oraclecloud.com:1521/service_name
DB_CONNECT_STRING=localhost:1521/XE

# Server Configuration
PORT=5000

# JWT Secret for authentication (generate a strong random string)
JWT_SECRET=your_jwt_secret_key_here

# Session configuration
SESSION_SECRET=your_session_secret_here
SESSION_EXPIRES=86400000 # 24 hours in milliseconds

# Uncomment below for development debugging
#NODE_ENV=development
#LOG_LEVEL=debug
